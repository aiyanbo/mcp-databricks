# Databricks Configuration
# Copy this file to .env and fill in your actual values

# Databricks workspace URL (e.g., https://your-workspace.databricks.com)
DATABRICKS_HOST=

# Databricks personal access token or service principal token
DATABRICKS_TOKEN=

# Databricks SQL warehouse HTTP path (e.g., /sql/1.0/warehouses/abc123def456)
DATABRICKS_HTTP_PATH=

# Default catalog name (optional, can be overridden in tool calls)
DATABRICKS_CATALOG=

# Default schema name (optional, can be overridden in tool calls)
DATABRICKS_SCHEMA=
